{
    "rewrites": [
        {
            "source": "/:locale/paginas/admin/painel",
            "destination": "/:locale/pages/admin/dashboard"
        },
        {
            "source": "/:locale/paginas/noticias",
            "destination": "/:locale/pages/news"
        },
        {
            "source": "/:locale/paginas/noticias/:cid",
            "destination": "/:locale/pages/news/:cid"
        },
        {
            "source": "/:locale/paginas/noticias/:cid/:id",
            "destination": "/:locale/pages/news/:cid/:id"
        },
        {
            "source": "/:locale/paginas/arquivo",
            "destination": "/:locale/pages/archive"
        },
        {
            "source": "/:locale/paginas/utilizadores",
            "destination": "/:locale/pages/users"
        }
    ]
}