{
    "rewrites": [
        {
            "id": 1,
            "locale": "pt-PT",
            "rules": [
                {
                    "source": "/pt-PT/paginas/admin/painel",
                    "destination": "/pt-PT/pages/admin/dashboard"
                },
                {
                    "source": "/pt-PT/paginas/noticias",
                    "destination": "/pt-PT/pages/news"
                },
                {
                    "source": "/pt-PT/paginas/noticias/:cid",
                    "destination": "/pt-PT/pages/news/:cid"
                },
                {
                    "source": "/pt-PT/paginas/noticias/:cid/:id",
                    "destination": "/pt-PT/pages/news/:cid/:id"
                },
                {
                    "source": "/pt-PT/paginas/etiquetas",
                    "destination": "/pt-PT/pages/tags"
                },
                {
                    "source": "/pt-PT/paginas/etiquetas/noticias",
                    "destination": "/pt-PT/pages/tags/news"
                },
                {
                    "source": "/pt-PT/paginas/etiquetas/noticias/:cid",
                    "destination": "/pt-PT/pages/tags/news/:cid"
                },
                {
                    "source": "/pt-PT/paginas/etiquetas/noticias/:cid/:tagname",
                    "destination": "/pt-PT/pages/tags/news/:cid/:tagname"
                },
                {
                    "source": "/pt-PT/paginas/arquivo",
                    "destination": "/pt-PT/pages/archive"
                },
                {
                    "source": "/pt-PT/paginas/utilizadores",
                    "destination": "/pt-PT/pages/users"
                }
            ]
        },
        {
            "id": 2,
            "locale": "fr-FR",
            "rules": [
                {
                    "source": "/fr-FR/pages/admin/panneau",
                    "destination": "/fr-FR/pages/admin/panel"
                },
                {
                    "source": "/fr-FR/pages/nouvelles",
                    "destination": "/fr-FR/pages/news"
                },
                {
                    "source": "/fr-FR/pages/nouvelles/:cid",
                    "destination": "/fr-FR/pages/news/:cid"
                },
                {
                    "source": "/fr-FR/pages/nouvelles/:cid/:id",
                    "destination": "/fr-FR/pages/news/:cid/:id"
                },
                {
                    "source": "/fr-FR/pages/balises",
                    "destination": "/fr-FR/pages/tags"
                },
                {
                    "source": "/fr-FR/pages/balises/nouvelles",
                    "destination": "/fr-FR/pages/tags/news"
                },
                {
                    "source": "/fr-FR/pages/balises/nouvelles/:cid",
                    "destination": "/fr-FR/pages/tags/news/:cid"
                },
                {
                    "source": "/fr-FR/pages/balises/nouvelles/:cid/:tagname",
                    "destination": "/fr-FR/pages/tags/news/:cid/:tagname"
                },
                {
                    "source": "/fr-FR/pages/archive",
                    "destination": "/fr-FR/pages/archive"
                },
                {
                    "source": "/fr-FR/pages/users",
                    "destination": "/fr-FR/pages/utilisateurs"
                }
            ]
        }
    ]
}